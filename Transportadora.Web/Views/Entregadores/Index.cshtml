@model IEnumerable<Transportadora.Model.Models.Entregadores>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Etnome)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Ettelefone)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Etnome)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Ettelefone)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Etcodigo">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Etcodigo">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Etcodigo">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
